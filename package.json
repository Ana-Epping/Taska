{
  "name": "Taska",
  "description": "Agenda multiplataforma",
  "author": "Ana Luiza Epping <luizaana16390@gmail.com>",
  "repository": "https://github.com/Ana-Epping/Taska",
  "build": {
    "appId": "br.edu.unoesc.taska",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@fullcalendar/core": "^5.11.0",
    "@fullcalendar/daygrid": "^5.11.0",
    "@fullcalendar/react": "^5.11.1",
    "@fullcalendar/interaction": "^5.11.0",
    "@fullcalendar/timegrid": "^5.11.0",
    "antd": "4.19.5",
    "concurrently": "7.1.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "electron-is-dev": "2.0.0",
    "eletron": "0.0.1-security",
    "express": "4.18.1",
    "fs": "0.0.1-security",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.0",
    "sequelize": "6.18.0",
    "sqlite3": "5.0.8",
    "wait-on": "6.0.1",
    "web-vitals": "2.1.4",
    "yarn": "1.22.18"
  },
  "devDependencies": {
    "electron": "^18.0.1",
    "electron-builder": "^22.14.13"
  },
  "main": "public/electron.js",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run build && electron-builder --linux --win --mac --x64 --ia32 --publish never",
    "build": "npm run react-build && npm run electron-build -- -p never",
    "publish": "npm run react-build && npm run electron-build -- -p always",
    "start": "concurrently -k \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron . \"",
    "lint": "eslint \"**/*.{j,t}s\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
